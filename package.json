{
	"name": "playgroundcode",
	"displayName": "PlaygroundCode",
	"description": "",
	"publisher": "yanzf",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.playgroundcode"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.playgroundcode",
				"title": "playgroundcode"
			},
			{
				"command": "extension.runplayground",
				"title": "Run Playground",
				"icon": {
					"dark": "./assets/icon/dark/run.png",
					"light": "$(play)"
				}
			}
		],
		"menus": {
			"editor/title": [
				{
					"when": "resourceScheme == playground",
					"command": "extension.runplayground",
					"group": "navigation"
				}
			]
		},
		"configuration": {
			"title": "Playground",
			"properties": {
				"playground.launch.go": {
					"type": "string",
					"default": "go"
				},
				"playground.launch.php": {
					"type": "string",
					"default": "php"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	},
	"dependencies": {
		"reflect-metadata": "^0.1.13"
	}
}
